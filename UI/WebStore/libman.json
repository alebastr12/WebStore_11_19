{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@2.2.0",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "provider": "unpkg",
      "library": "bootstrap@3.3.7",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "dist/css/bootstrap-theme.css",
        "dist/css/bootstrap-theme.css.map",
        "dist/css/bootstrap-theme.min.css",
        "dist/css/bootstrap.css",
        "dist/css/bootstrap.css.map",
        "dist/css/bootstrap.min.css",
        "dist/fonts/glyphicons-halflings-regular.eot",
        "dist/fonts/glyphicons-halflings-regular.svg",
        "dist/fonts/glyphicons-halflings-regular.ttf",
        "dist/fonts/glyphicons-halflings-regular.woff",
        "dist/js/bootstrap.js",
        "dist/js/bootstrap.min.js"
      ]
    },
    {
      "library": "font-awesome@4.0.3",
      "destination": "wwwroot/lib/font-awesome/",
      "files": [
        "css/font-awesome.css",
        "css/font-awesome.min.css",
        "fonts/fontawesome-webfont.eot",
        "fonts/fontawesome-webfont.svg",
        "fonts/fontawesome-webfont.ttf",
        "fonts/fontawesome-webfont.woff",
        "fonts/FontAwesome.otf"
      ]
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/"
    },
    {
      "library": "jquery-validate@1.19.0",
      "destination": "wwwroot/lib/jquery-validate/",
      "files": [
        "jquery.validate.min.js",
        "localization/messages_ru.min.js",
        "jquery.validate.js",
        "additional-methods.min.js",
        "additional-methods.js"
      ]
    },
    {
      "library": "prettyPhoto@3.1.6",
      "destination": "wwwroot/lib/prettyPhoto/"
    },
    {
      "library": "jquery-loading-overlay@2.1.6",
      "destination": "wwwroot/lib/jquery-loading-overlay/"
    },
    {
      "provider": "unpkg",
      "library": "jquery-ajax-unobtrusive@3.2.6",
      "destination": "wwwroot/lib/jquery-ajax-unobtrusive/",
      "files": [
        "dist/jquery.unobtrusive-ajax.js",
        "dist/jquery.unobtrusive-ajax.min.js"
      ]
    },
    {
      "provider": "unpkg",
      "library": "@aspnet/signalr@1.1.4",
      "destination": "wwwroot/lib/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.js.map",
        "dist/browser/signalr.min.js",
        "dist/browser/signalr.min.js.map"
      ]
    }
  ]
}